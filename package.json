{
    "name": "squirrelfcmrelay",
    "version": "1.0.0",
    "description": "A simple firebase cloud messaging relay server",
    "main": "index.js",
    "scripts": {
        "tsc": "tsc",
        "start": "set debug=* && set NODE_ENV=development && ts-node-dev ./index.ts",
        "prod": "tsc && node ./build/index.js",
        "build": "tsc",
        "lint": "eslint -c .eslintrc.json --ext .ts .",
        "fix": "eslint -c .eslintrc.json --ext .ts . --fix"
    },
    "keywords": [
        "relay",
        "server",
        "firebase",
        "cloud",
        "fcm"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SchoolSquirrel/SquirrelFcmRelay.git"
    },
    "author": "SchoolSquirrel Team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SchoolSquirrel/SquirrelFcmRelay/issues"
    },
    "homepage": "https://github.com/SchoolSquirrel/SquirrelFcmRelay#readme",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "firebase-admin": "^11.4.1",
        "helmet": "^4.1.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.7",
        "@types/express": "^4.17.7",
        "@types/helmet": "0.0.47",
        "@types/node": "^14.6.0",
        "@typescript-eslint/eslint-plugin": "^3.9.1",
        "@typescript-eslint/parser": "^3.9.1",
        "eslint": "^7.7.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-typescript": "^2.2.1",
        "eslint-plugin-github": "^4.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-prettier": "^3.1.4",
        "ts-node-dev": "^1.0.0-pre.58",
        "typescript": "^3.9.7"
    }
}